{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "type",
      "title",
      "body"
    ],
    "properties": {
      "type": {
        "enum": [
          "ok",
          "warn"
        ]
      },
      "title": {
        "type": "string"
      },
      "body": {
        "type": "string"
      }
    }
  }
}